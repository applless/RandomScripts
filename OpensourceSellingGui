local plr = game:GetService("Players").LocalPlayer
local Red = Color3.fromRGB(255,0,0)
local Green = Color3.fromRGB(0,255,0)


--// Applys UIStrokes cuz Gui to Lua doesnt convert them lol 
local Frames = {PredroppedFrame,AdminbaseButton,BankButton,BasketButton,ClubButton,SchoolButton,AdminjailButton,FlyButton,FpsButton,FpsInput,LowGfxButton,NoclipButton,LocationsButton,MainButton,TargetButton,TogglesButton,GoToButton,TargetInput,ViewButton,WalletFrame,AntigrabButton,AntistompButton,AutodropButton,CashauraButton}

for i,v in pairs(Frames) do 
	local Stroke = Instance.new("UIStroke",v)
	Stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
	Stroke.Color = Color3.fromRGB(0,0,0)
	Stroke.Thickness = 1
	Stroke.Transparency = 0.7
end
--\\


--// Tab Toggles
LocationsButton.MouseButton1Click:Connect(function()
	LocationsFrame.Visible = true
	MainFrame.Visible = false
	TargetFrame.Visible = false
	TogglesFrame.Visible = false
end)

MainButton.MouseButton1Click:Connect(function()
	LocationsFrame.Visible = false
	MainFrame.Visible = true
	TargetFrame.Visible = false
	TogglesFrame.Visible = false
end)

TargetButton.MouseButton1Click:Connect(function()
	LocationsFrame.Visible = false
	MainFrame.Visible = false
	TargetFrame.Visible = true
	TogglesFrame.Visible = false
end)

TogglesButton.MouseButton1Click:Connect(function()
	LocationsFrame.Visible = false
	MainFrame.Visible = false
	TargetFrame.Visible = false
	TogglesFrame.Visible = true
end)
--\\


--// Toggle [V]
plr:GetMouse().KeyDown:Connect(function(PressedKey)
	if PressedKey == "v" then 
		Main.Visible = not Main.Visible
	end
end)
--\\ 


--// Drag
local UserInputService = game:GetService("UserInputService")

local gui = Main

local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)
