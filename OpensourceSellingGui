--// Noclip toggle
local NoclipActive = false
NoclipButton.MouseButton1Click:Connect(function()
	plr:GetMouse().KeyDown:Connect(function(KeyPressed)
		if KeyPressed == "x" then 
			NoclipActive = not NoclipActive
			while true do 
				if NoclipActive == true and plr.Character then 
					for i,v in pairs(plr.Character:GetDescendants()) do
						pcall(function()
							if v:IsA("BasePart") then
								v.CanCollide = false
							end
						end)
					end
				end
				game:GetService("RunService").Stepped:wait()
			end
		end
	end)
end)
--\\


--// uhh idk convert to money thingy 
local z = (function (n)
	n = tostring(n)
	return n:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
end)
--\\



game:GetService("RunService").Stepped:Connect(function()
	
	
	--// Targets wallet
	if TargetInput.Text ~= "" then 
		if game:GetService("Players"):FindFirstChild(TargetInput.Text) then 
			local Amount = game:GetService("Players"):FindFirstChild(TargetInput.Text).DataFolder.Currency.Value
			Wallet.Text = z(tonumber(Amount)).."$"
		end
	end
	--\\
	
	
	--// Antigrab
	if AntigrabButton.TextColor3 == Green then 
		if plr.Character:FindFirstChild("GRABBING_CONSTRAINT") then 
			plr.Character:FindFirstChild("GRABBING_CONSTRAINT"):Destroy() wait(.1)
			plr.Character:FindFirstChildWhichIsA("Humanoid").Sit = true 
		end
	end
	--\\
	
	
	--// Antistomp
	if AntistompButton.TextColor3 == Green then
	if game.Players.LocalPlayer.Character.BodyEffects:FindFirstChild("K.O").Value == true then
			game.Players.LocalPlayer.Character.Humanoid.Health = 0
			for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
				if v.ClassName == "Tool" then v.Parent = game.Players.LocalPlayer.Backpack
				end
			end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
			if v.ClassName == "Humanoid" or v.ClassName == "MeshPart" or v.ClassName == "Part" or v.ClassName == "Accessory"then
				v:Destroy() 
				end
			end
		end
	end
	--\\
	
	
	--// Autodrop
	if AutodropButton.TextColor3 == Green then
		game:GetService("ReplicatedStorage").MainEvent:FireServer("DropMoney","10000")
	end
	--\\
	
	
	--// Cashaura
	if CashauraButton.TextColor3 == Green then 
		for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
			if v:IsA('Part') then
				if (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 13.8 then
					fireclickdetector(v:FindFirstChild('ClickDetector'))
				end
			end
		end
	end
	--\\
	

	--// Predropped
	local amount = 0
	for i,v in pairs(game.Workspace.Ignored.Drop:GetChildren()) do
		if v.Name == "MoneyDrop" then
			local cash = string.split(v.BillboardGui.TextLabel.Text,"$")[2]
			if string.match(cash,',') then
				amount = amount + cash:gsub("%,","")
			else
				amount = amount + cash
			end
		end
	end
	local function checking(amount)
		amount = tostring(amount)
		return amount:reverse():gsub("%d%d%d", "%1,"):reverse():gsub("^,", "")
	end
	Predropped.Text = "PD: "..checking(amount).."$"
	--\\
	
	
end)



--// TargetInput autofill username
TargetInput.FocusLost:connect(function()
	for i,v in pairs(game.Players:GetChildren()) do
		if (string.sub(string.lower(v.Name),1,string.len(TargetInput.Text))) == string.lower(TargetInput.Text) then
			TargetInput.Text = v.Name
		end
	end
end)
--\\


--// GoTo 
GoToButton.MouseButton1Click:Connect(function()
	if TargetInput.Text ~= "" then 
		if game:GetService("Players"):FindFirstChild(TargetInput.Text) then 
			plr.Character:FindFirstChild("HumanoidRootPart").CFrame = game:GetService("Players"):FindFirstChild(TargetInput.Text).Character.Head.CFrame 
		end
	end
end)
--\\


--// View
ViewButton.MouseButton1Click:Connect(function()
	if TargetInput.Text ~= "" then 
		if game:GetService("Players"):FindFirstChild(TargetInput.Text) then 
			if ViewButton.Text == "View" 
				then ViewButton.Text = "Unview"
				game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = game:GetService("Players"):FindFirstChild(TargetInput.Text).Character:FindFirstChildWhichIsA("Humanoid")
			else ViewButton.Text = "View"
				game.Workspace:FindFirstChildWhichIsA('Camera').CameraSubject = plr.Character:FindFirstChildWhichIsA("Humanoid")
			end	
		end
	end
end)
--\\


